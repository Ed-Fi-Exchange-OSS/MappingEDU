<package>
    <metadata>
        <id>MappingEdu.Web.UI</id>
        <version>1.0.0.0</version>
        <authors>Ed-Fi Alliance</authors>
        <owners>Ed-Fi Alliance</owners>
        <license type="file">LICENSE</license>
        <projectUrl>https://github.com/Ed-Fi-Exchange-OSS/MappingEDU</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>The MappingEDU Application Deployment Package</description>
        <releaseNotes></releaseNotes>
    </metadata>
    <files>
        <file src="bin\**\*.*" target="bin" exclude="*.nupkg;**/roslyn/*"/>
        <file src="Client/*.js" target="client" />
        <file src="Client/Content/**/*.*" target="client/Content"
            exclude="Client/Content/styles/bootstrap.min.css;Client/Content/styles/font-awesome.min.css;Client/Content/styles/bootstrap-theme.min.cs" />
        <file src="Favicon.ico" target="Favicon.ico" />
        <file src="Views\**\*.*" target="Views"/>
        <file src="Templates\**\*.*" target="Templates"/>
        <file src="Web.config" target="Web.config" />
        <file src="..\packages\EntityFramework.6.2.0\tools\migrate.exe" target="bin" />
        <file src="../../LICENSE" />
        <file src="../../NOTICES.md" />
    </files>
</package>
